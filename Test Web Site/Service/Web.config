<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
	<system.serviceModel>
			<behaviors>
			<endpointBehaviors>
				<behavior name="AjaxBehavior">
					<enableWebScript/>
				</behavior>
			</endpointBehaviors>
		</behaviors>
		<services>
			<service name="RedStapler.TestWebSite.Service.Service">
				<endpoint address="" behaviorConfiguration="AjaxBehavior" binding="webHttpBinding" contract="RedStapler.TestWebSite.Service.IService"/>
			</service>
		</services>
	</system.serviceModel>
</configuration>
